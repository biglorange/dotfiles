	name = biglorange
[alias]
	st = status
	sts = stash
	stp = stash pop
	co = checkout
	cob = checkout -b
	br = branch
	last = log -1
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
	cp = cherry-pick
	unsta = reset HEAD
[core]
	excludesfile = ~/.gitignore
[pull]
	rebase = true
[include]
	path = ~/.gitconfig_ext
